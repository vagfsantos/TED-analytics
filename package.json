{
  "name": "ted-analytics",
  "version": "1.0.0",
  "description": "TED Analytics",
  "repository": "https://github.com/vagfsantos/TED-analytics.git",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "dev": "cross-env development=true APP_NAME=TED_ANALYTICS nodemon --inspect --watch config --watch server server/main.js",
    "build": "cross-env production=true APP_NAME=TED_ANALYTICS webpack --config=config/webpack.prod.js",
    "build:dev": "cross-env development=true APP_NAME=TED_ANALYTICS webpack --config=config/webpack.dev.js",
    "prod": "yarn build && cross-env production=true node server/main.js"
  },
